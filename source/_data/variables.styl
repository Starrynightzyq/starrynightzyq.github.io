// 标题，修改成你期望的字体族
$font-family-headings = "Microsoft YaHei", Verdana, sans-serif

// 修改成你期望的字体族
$font-family-base = "Microsoft YaHei", Verdana, sans-serif

// 代码字体
$code-font-family = "Input Mono", "PT Mono", Consolas, Monaco, Menlo, monospace

// 正文字体的大小
$font-size-base = 14px

// 代码字体的大小
$code-font-size = 11px